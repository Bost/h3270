    <build>
        <nagEmailAddress>dev@maven.apache.org</nagEmailAddress>
        <sourceDirectory>src</sourceDirectory>
        <unitTestSourceDirectory>test/src</unitTestSourceDirectory>
        <unitTest>
            <includes>
                <include>**/*Test.java</include>
            </includes>
            <excludes>
                <exclude>**/RepositoryTest.java</exclude>
                <exclude>**/AllTest.java</exclude>
                <exclude>**/Abstract*.java</exclude>
                <exclude>**/FileScreenTest.java</exclude>
            </excludes>
            <resources>
                <resource>
                    <directory>${basedir}/test/src</directory>
                    <includes>
                        <include>**/*.txt</include>
                        <include>**/*.dump</include>
                    </includes>
                    <filtering>false</filtering>
                </resource>
                <resource>
                    <directory>${basedir}/webapp</directory>
                    <includes>
                        <include>**/*</include>
                    </includes>
                    <filtering>false</filtering>
                </resource>
            </resources>
        </unitTest>
    </build>
    <properties/>
</project>